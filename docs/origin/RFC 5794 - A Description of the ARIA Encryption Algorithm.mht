       padAlgo   AriaPadAlgo  DEFAULT specifiedPadAlgo:id-pad-null
   }

   AriaCbcParameters ::=3D SEQUENCE {
       m         INTEGER       DEFAULT 1,
       -- number of stored ciphertext blocks
       padAlgo   AriaPadAlgo   DEFAULT specifiedPadAlgo:id-pad-1
   }

   AriaCfbParameters ::=3D SEQUENCE {
       r         INTEGER,
       -- bit-length of feedback buffer, 128&lt;=3Dr&lt;=3D128*1024
       k         INTEGER,
       -- bit-length of feedback variable, 1&lt;=3Dk&lt;=3D128
       j         INTEGER,
       -- bit-length of plaintext/ciphertext block, 1&lt;=3Dj&lt;=3Dk
       padAlgo   AriaPadAlgo  DEFAULT specifiedPadAlgo:id-pad-null
   }

   AriaOfbParameters ::=3D SEQUENCE {
       j         INTEGER,
       -- bit-length of plaintext/ciphertext block, 1&lt;=3Dj&lt;=3D128
       padAlgo   AriaPadAlgo  DEFAULT specifiedPadAlgo:id-pad-null
   }

   AriaCtrParameters ::=3D SEQUENCE {
       j         INTEGER,
       -- bit-length of plaintext/ciphertext block, 1&lt;=3Dj&lt;=3D128
       padAlgo   AriaPadAlgo  DEFAULT specifiedPadAlgo:id-pad-null
   }

